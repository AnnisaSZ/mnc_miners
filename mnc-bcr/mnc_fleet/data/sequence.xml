<?xml version="1.0"?>
<odoo>
    <data>
        <!-- Sequence -->
        <record id="seq_breakdown_production" model="ir.sequence">
            <field name="name">Breakdown Fleet</field>
            <field name="code">break.down</field>
            <field name="company_id" eval="False"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field name="suffix">/BD-INFRA/MONTH/%(year)s</field>
        </record>
        <record id="seq_hourly_production" model="ir.sequence">
            <field name="name">Hourly Production</field>
            <field name="code">hourly.production</field>
            <field name="company_id" eval="False"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field name="suffix">/HP-INFRA/MONTH/%(year)s</field>
        </record>
        <record id="seq_shift_production" model="ir.sequence">
            <field name="name">Shift Production</field>
            <field name="code">shift.production</field>
            <field name="company_id" eval="False"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
            <field name="suffix">/SP-INFRA/MONTH/%(year)s</field>
        </record>
    </data>
</odoo>