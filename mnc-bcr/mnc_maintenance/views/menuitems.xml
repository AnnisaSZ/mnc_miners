<?xml version="1.0"?>
<odoo>
    <!-- Menuitems -->
    <menuitem id="mncei_warehouse_apps"
        name="Warehouse"
        active="True"
        web_icon="mnc_maintenance,static/description/icon.png"
        sequence="10"/>

    <!-- Stock -->
    <menuitem id="warehouse" parent="mncei_warehouse_apps" sequence="1" name="Inventory"/>
    <menuitem id="sparepart_list" action="mnc_maintenance.action_spare_part" parent="warehouse" sequence="1" name="Spare Part"/>
    <!-- Operations -->
    <menuitem id="operations" parent="mncei_warehouse_apps" sequence="10" name="Operations"/>
    <menuitem id="transfer_sparepart" action="mnc_maintenance.action_picking_tree_all_sparepart" parent="operations" sequence="1" name="Transfer"/>
    <menuitem id="adjustment" action="mnc_maintenance.action_inventory_form_sparepart" parent="operations" sequence="1" name="Inventory Adjistments"/>
    <!-- Reporting -->
    <menuitem id="report" parent="mncei_warehouse_apps" sequence="20" name="Reporting"/>
    <menuitem id="product_moves_sparepart" action="mnc_maintenance.stock_move_line_action_sparepart" parent="report" sequence="1" name="Product Moves"/>
    <menuitem id="product_valuation_sparepart" action="mnc_maintenance.stock_valuation_layer_action_sparepart" parent="report" sequence="10" name="Product Valuation"/>
    <!-- Configuration -->
    <menuitem id="configuration" parent="mncei_warehouse_apps" sequence="100" name="Configuration"/>
    <menuitem id="warehouse_datas" action="stock.action_warehouse_form" parent="configuration" sequence="1" name="Warehouse"/>
    <menuitem id="product_categ" action="mnc_maintenance.product_category_action_form_sparepart" parent="configuration" sequence="10" name="Product Categories"/>
    <!-- UoM -->
    <menuitem id="unit_measure" parent="configuration" sequence="100" name="Unit Of Measure"/>
    <menuitem id="uom_categ_settings" action="uom.product_uom_categ_form_action" parent="unit_measure" sequence="1" name="UoM Categories"/>
    <menuitem id="uom_settings" action="uom.product_uom_form_action" parent="unit_measure" sequence="2" name="UoM"/>

</odoo>