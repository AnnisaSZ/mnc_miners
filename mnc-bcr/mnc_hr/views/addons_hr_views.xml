<?xml version="1.0"?>
<odoo>
	<!-- Department -->
	<record id="mncei_department_form" model="ir.ui.view">
        <field name="name">mncei.department.form</field>
        <field name="model">mncei.department</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="id_department"/>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="state"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="mncei_department_tree" model="ir.ui.view">
        <field name="name">mncei.department.tree</field>
        <field name="model">mncei.department</field>
        <field name="arch" type="xml">
            <tree>
                <field name="id_department" widget="handle"/>
                <field name="name"/>
                <field name="state"/>
            </tree>
        </field>
    </record>

    <record id="mncei_department_actions" model="ir.actions.act_window">
        <field name="name">Department</field>
        <field name="res_model">mncei.department</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your department
            </p>
        </field>
    </record>

