<?xml version="1.0"?>
<odoo>
    <data noupdate="1">
        <record id="training_bspc_seq" model="ir.sequence">
            <field name="name">Code BSPC Training</field>
            <field name="code">training.code</field>
            <field name="suffix">/BSPC/%(day)s/%(month)s/%(y)s</field>
            <field eval="3" name="padding"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
