        <record id="formview_master_tugboat" model="ir.ui.view">
            <field name="name">formview_master_tugboat</field>
            <field name="model">master.tugboat</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form delete="0">
                    <sheet>
                        <group>
