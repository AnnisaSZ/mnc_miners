<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Datas -->
    <data noupdate="1">
        <record id="it_subs_seq" model="ir.sequence">
            <field name="name">IT Subs Sequence</field>
            <field name="code">it.subs.form</field>
            <field name="implementation">no_gap</field>
            <field name="suffix">/IT/MNCEI/CUSTOMCODE/%(year)s</field>
            <field eval="3" name="padding"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>

